{
	// Place your Web__Exemple workspace snippets here. Each snippet is defined under a snippet name and has a scope, prefix, body and
	// description. Add comma separated ids of the languages where the snippet is applicable in the scope field. If scope
	// is left empty or omitted, the snippet gets applied to all languages. The prefix is what is
	// used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders.
	// Placeholders with the same ids are connected.
	// Example:
"Print to console": {
	"scope": "css,html",   // tipo 
 	"prefix": "color",  //atalho 
		"body": [
		"    background: $1;",  // $num define a ordem tipo curso comessa no $1 enter vai pro $2
		"    color:$2;"
		],
		"description": "Log output to console" // descricao pra q serve
	 },
     //parA add um snipper  { pri..{ snipper [ code ], },  pri..{ snipper [ code ], }  }

"Print to console": {
	"scope": "html,xml",   // tipo 
 	"prefix": "come",  //atalho 
		"body": [
		"   <!-- $1-->", 
        "   /* $2 */ "
		],
		"description": "Log output to console" // descricao pra q serve
	 }
     
}

